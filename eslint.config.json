{
  "plugins": [
    "react"
  ],

  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "no-trailing-spaces": 2,
    "eol-last": 2,
    "no-underscore-dangle": 0,
    "no-alert": 2,
    "no-lone-blocks": 0,
    "no-bitwise": 2,
    "camelcase": 2,
    "curly": 2,
    "eqeqeq": 2,
    "wrap-iife": [2,"any"],
    "indent": [2,2,{"SwitchCase": 1}],
    "no-use-before-define": 2,
    "new-cap": 2,
    "no-caller": 2,
    "quotes": [2,"single"],
    "no-undef": 2,
    "no-unused-vars": 2,
    "strict": 0,//[2,"function"]
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1
  },
  "globals": {
    jQuery: true,
    $: true
  }
}